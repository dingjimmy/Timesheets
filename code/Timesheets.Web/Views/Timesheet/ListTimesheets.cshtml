@using Timesheets.Web.Models
@model IEnumerable<TimesheetViewModel>
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="list-group">

    <partial name="Molecules/TimesheetListSearch" />

    <partial name="Molecules/TimesheetListAdd" />   

    @foreach (TimesheetViewModel ts in Model)
    {
        <partial name="Molecules/TimesheetListItem" model="@ts" />
    }
</div>