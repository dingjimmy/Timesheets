@using Timesheets.Web.Models
@model TimesheetViewModel

<a class="list-group-item" hx-get="/timesheets/@Model.ID" hx-target="#timesheet-detail" href="/timesheet/@Model.ID">
    <div>
        <h5 class="mb-1">@Model.Name</h5>
    </div>
    <p class="mb-1">@Model.PeriodStarts.ToLongDateString() to @Model.PeriodEnds.ToLongDateString()</p>
</a>